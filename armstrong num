import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		int sum=0;
		int temp=n;
		while(n>0){
		    int rem=n%10;
		    sum=sum+(rem*rem*rem);
		    n/=10;
		}
		if(temp==sum){
		    System.out.println("armstrong num");
		}
		else
		{
		    System.out.println("not");
		}
	}
}
