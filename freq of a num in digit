import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		    int n=s.nextInt();
		    int d=s.nextInt();
		    int count=0;
		    while(n>0){
		        int rem=n%10;
		        if(rem==d){
		        count++;
		        }
		        n=n/10;
		    }
		System.out.println(count);
	}
}
