import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s =new Scanner(System.in);
		int a=s.nextInt();
		int temp=a;
		int sum=0;
		int sq=a*a;
		while(sq>0){
		    int rem=sq%10;
		    sum=sum+rem;
		    sq/=10;
		}
		System.out.println(sum);
		if(sum==temp){
		System.out.println("given is a neon number");
		}
		else{
		System.out.println("given is not a neon number");

		}
	}
}
