import java.util.Scanner;
public class Main {
 public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int sum1=0;
int sum2=0;
while(n>0){
    int rem=n%10;
    if(rem%2==0)
        sum1=sum1+rem;
    else
        sum2=sum2+rem;
    n/=10;
}
 int diff=sum1-sum2;
 System.out.println("difference is: "+diff);
  }
     
}
