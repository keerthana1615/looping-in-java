import java.util.Scanner;
public class Main {
 public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int ec=0;
int oc=0;
while(n>0){
    int rem=n%10;
    if(rem%2==0)
        ec++;
    else
    oc++;
    n/=10;
}
if(ec>=0 && oc==0){
 System.out.print("even");
}
else if(ec==0 && oc>=0){
 System.out.print("odd");
}
else {
 System.out.print("mixed");
}
  }
     
}
