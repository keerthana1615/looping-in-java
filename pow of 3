import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int b=1;
    for(int i=1;i*i*i<=n;i++){
          b=i*i*i;
          if(b==n){
         System.out.println("it is pow of 3");
          }
          
    }
    if(b!=n){
                     System.out.println("not a pow of 3"); 
    }
  }
     
}
