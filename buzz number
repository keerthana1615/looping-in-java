import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
    int rem=n%10;
    if(rem==7||n%7==0){
        System.out.println("it is a buzz number");
    }
    else{
         System.out.println("it is not a buzz number");
    }
     
  }
}
