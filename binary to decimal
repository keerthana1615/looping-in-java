import java.util.Scanner;
class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
          int b = sc.nextInt();
        int decimal = 0, power = 1;
        while (b > 0) {
            int digit = b % 10;
            decimal = decimal + digit * power;
            power = power * 2;
            b = b / 10;
        }
        System.out.println("Decimal = " + decimal);
    }
}
