import java.util.Scanner;
public class Main {
 public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int sum=0;
int place=1;
while(n>0){
    int rem=n%10;
  int  sq=rem*rem;
  int cu=rem*rem*rem;
  sum=sum+sq+cu;
    n=n/10;
}

 System.out.print(sum);
  }
     
}
