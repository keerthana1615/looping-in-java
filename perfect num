import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		int sum=0;
		int temp=n;
		    for(int i=1;i<n;i++){
		       if(n%i==0) {
		    sum+=i;
		       }
		    }
		    if(sum==n){
		    System.out.println("perfect num");
		    }
		else
		{
		    System.out.println("not");
		}
	}
}
