import java.util.Scanner;
class Main {
    public static void main(String args[]) {
        Scanner s = new Scanner(System.in);
         int n=s.nextInt();
         int count=0;
         for(int i=2;count<n;i++){
             int c=0;
             for(int j=1;j<=i;j++){
                 if(i%j==0)
                 c++;
             }
         
         if(c==2){
             System.out.print(i+" ");
             count++;
         }
         
         }
    }
}
