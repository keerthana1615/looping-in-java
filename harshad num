import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		int sum=0;
		int temp=n;
		while(n>0){
		   int rem = n%10;
		   sum+=rem;
		   n/=10;
		}
		   if(temp%sum==0){
		       System.out.println("harshad num");
		   }
		   else{
		       System.out.println("not an harshad num");
		   }
	}
}
