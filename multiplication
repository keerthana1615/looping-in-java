import java.util.Scanner;
class Main {
    public static void main(String args[]) {
        Scanner s = new Scanner(System.in);
         int n=s.nextInt();
         int b;
         for(int i=1;i<=10;i++){
             b=n*i;
             System.out.println(n+" "+"*"+" "+i+" = "+b);
         }
    }
}
