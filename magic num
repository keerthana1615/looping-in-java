import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int n=s.nextInt();
		while(n>9)
		{
		  int sum=0;
		  while(n>0){
		      sum = sum+(n%10);
		      n=n/10;
		  }
		  n=sum;
		}
		if (n == 1)
            System.out.println("Magic number");
        else
            System.out.println("Not a magic number");
	}
}
