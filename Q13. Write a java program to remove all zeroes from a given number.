import java.util.Scanner;
public class Main {
 public static void main(String[] args) {
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int ne=0;
int place=1;
while(n>0){
    int rem=n%10;
    if(rem!=0){
    ne=ne+rem*place;
    place=place*10;
    } 
    n/=10;
}
 System.out.print(ne);
  }
     
}
