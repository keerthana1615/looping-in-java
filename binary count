import java.util.Scanner;
class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        String b = sc.next();
        int i, zero = 0, one = 0;
        for (i = 0; i < b.length(); i++) {
            if (b.charAt(i) == '0')
                zero++;
            else
                one++;
        }
        System.out.println("0s = " + zero);
        System.out.println("1s = " + one);
    }
}
